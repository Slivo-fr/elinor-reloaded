<Window x:Class="Elinor.ProfileNameWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="Enter new profile name" Height="160" Width="387"
        ResizeMode="CanMinimize" Loaded="WindowLoaded" Icon="/Elinor;component/Images/mainicon.png" KeyDown="WindowKeyDown">
  <Grid>
    <Label Content="Select profile name" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1"
           VerticalAlignment="Top" />
    <TextBox Height="23" Margin="12,46,12,0" Name="tbName" VerticalAlignment="Top" TextChanged="TbNameTextChanged" />
    <Button Content="OK" Height="23" HorizontalAlignment="Right" Margin="0,0,12,12" Name="btnOk"
            VerticalAlignment="Bottom" Width="75" Click="BtnOkClick" IsEnabled="False" />
    <Button Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="0,0,93,12" Name="btnCancel"
            VerticalAlignment="Bottom" Width="75" Click="BtnCancelClick" />
  </Grid>
</Window>